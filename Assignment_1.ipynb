    "Q2 Answer\n",
    "\n",
    "total = 0\n",
    "for number in range(1,11):\n",
    "    total += number\n",
    "print(total)\n",
    "\n",
    "\n",
    "total = 0\n",
    "number = 1\n",
    "while(number < 11):\n",
    "    total += number\n",
    "    number +=1\n",
    "print(total)    "
   ]
